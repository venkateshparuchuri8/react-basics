{
  "name": "react-basics",
  "version": "1.0.0",
  "description": "Some Basic React Js",
  "main": "index.js",
  "scripts": {
    "singlePath": "rc-autocomplete --src 'node_modules/react-toolbox/components/**/*.js'",
    "generate": "rc-autocomplete",
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --port 8081",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "Dishari De",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "reactstrap": "^5.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "i": "^0.3.5",
    "react-autocomplete-cli": "0.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3"
  }
}
